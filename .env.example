# .env.example
# 실제 개발/운영 환경에서는 .env 파일에 아래 값을 각각 설정하세요.

# PostgreSQL 연결 문자열 예시
#   DB_USER:   postgres (또는 dev_user 등)
#   DB_PASSWORD:  <자신 PC/개발용 DB 비밀번호>
#   DB_NAME:   correction (예시)
DATABASE_URL=postgresql://<DB_USER>:<DB_PASSWORD>@localhost:5432/<DB_NAME>

# Firebase 서비스 계정 키 경로 예시
#   실제 경로는 로컬마다 다르므로 플레이스홀더 형태로 남겨주세요.
FIREBASE_CREDENTIAL_PATH=./path/to/firebase-service-account.json

# .env 예시
JWT_SECRET_KEY=<your_jwt_secret_key> #담당자에게 문의
JWT_ALGORITHM=HS256
JWT_ACCESS_EXPIRE_MINUTES=60

#LLM API 키 연결 예시 (현재 LLM GEMINI 사용)
GEMINI_API_KEY="여기에_API_키_붙여넣기"